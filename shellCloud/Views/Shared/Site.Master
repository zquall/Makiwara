			<!--  start account-content -->
            <% Html.RenderPartial("LogOnUserControl"); %>			
			<!--  end account-content -->
		
		</div>
		<!-- end nav-right -->
		<!--  start nav -->
		<div class="nav">
		</div>
		<!--  start nav -->

</div>
<!--  start nav-outer -->
</div>
<!--  start nav-outer-repeat................................................... END -->


    <div class="page">
        <div id="header">
            <div id="title">
                
            </div>
            <div id="logindisplay">
              
            </div> 
            <div id="menucontainer">
                <ul id="menu">
                    <li><%: Html.ActionLink("Inicio", "Index", "Home")%></li>
                    <li><%: Html.ActionLink("Estado", "About", "Home")%></li>
