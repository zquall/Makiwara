	        <h1>SkyNet Cloud Services</h1>
            <h3>www.hosting-w3.com</h3>
	    </div>
	    <!-- end logo -->
	
 	    <div class="clear"></div>

    </div>
    <!-- End: page-top -->

</div>
<!-- End: page-top-outer -->
<div class="clear">&nbsp;</div>

<!--  start nav-outer-repeat................................................................................................. START -->
<div class="nav-outer-repeat"> 
<!--  start nav-outer -->
<div class="nav-outer"> 

