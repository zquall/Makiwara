    <link href="../../Content/css/main.css" rel="stylesheet" type="text/css" />
    <!--  JQquery core -->
    <script src="<%: Url.Content("~/Scripts/jquery-1.5.1.min.js") %>" type="text/javascript"></script>

    <!-- Custom jquery scripts -->
